{
  "name": "react-typescript-arsenal-tools",
  "version": "0.1.0",
  "private": true,
  "description": "AI-powered analysis tools for React/TypeScript projects with cross-platform support",
  "keywords": [
    "react",
    "typescript",
    "analysis",
    "ai",
    "claude",
    "react-native",
    "monorepo"
  ],
  "author": "Your Name",
  "license": "MIT",
  "packageManager": "pnpm@8.15.0",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev --parallel",
    "lint": "turbo lint",
    "test": "turbo test",
    "type-check": "turbo type-check",
    "clean": "turbo clean && rm -rf node_modules",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "release": "pnpm build && changeset publish",
    "dev:cli": "pnpm --filter @rta/cli dev",
    "dev:webhook": "pnpm --filter webhook-service dev",
    "dev:dashboard": "pnpm --filter web-dashboard dev",
    "demo": "pnpm build && tsx scripts/demo.ts",
    "demo:keep": "pnpm build && tsx scripts/demo.ts --keep",
    "setup": "pnpm install && pnpm build",
    "rta": "node packages/cli/dist/index.js"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.4",
    "@types/node": "^20.17.52",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "chalk": "^5.4.1",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.4.0",
    "ora": "^7.0.1",
    "prettier": "^3.5.3",
    "tsup": "^8.5.0",
    "tsx": "^4.19.4",
    "turbo": "^2.5.3",
    "typescript": "^5.8.3",
    "vitest": "^1.6.1"
  },
  "pnpm": {
    "overrides": {
      "typescript": "^5.8.3",
      "@typescript-eslint/parser": "^7.18.0",
      "@typescript-eslint/eslint-plugin": "^7.18.0"
    }
  }
}
